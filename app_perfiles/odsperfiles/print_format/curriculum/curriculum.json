 "html": "<meta name=\"pdfkit-page-size\" content=\"Legal\"/>\n<meta name=\"pdfkit-orientation\" content=\"portrait\"/>\n\n{%set datos_generales = frappe.get_doc(\"Datos Generales\",doc.id_participante) %}\n{%set curso = frappe.get_doc(\"Curso\",doc.codigocurso) %}\n{%set logos = frappe.get_doc(\"Logos\",doc.metodologia) %}\n{%set titulo = frappe.db.get_list('Formacion',\n    filters={\n        'numero_id_fo': doc.id_participante\n    },\n    fields=['titulo', 'institucion','fecha','nivel_formativo'],\n    order_by='titulo desc'\n)%}\n{%set conocimiento = frappe.db.get_list('Conocimiento',\n    filters={\n        'numero_id_co': doc.id_participante\n    },\n    fields=['titulo', 'institucion','fecha','horas'],\n    order_by='titulo desc'\n)%}\n{%set experiencia = frappe.db.get_list('Experiencia Laboral',\n    filters={\n        'numero_id_el': doc.id_participante\n    },\n    fields=['tipo', 'posicion','empresa','personas_acargo','fecha_ingreso','fecha_salida','meses','descripcion'],\n    order_by='tipo desc'\n)%}\n{%set idioma = frappe.db.get_list('Idioma',\n    filters={\n        'numero_id_id': doc.id_participante\n    },\n    fields=['idioma', 'lectura','escrito','hablado'],\n    order_by='idioma desc'\n)%}\n <div class=\"print-heading\">\n    <table style=\"width:100% \"  >\n                <tr>\n                    <td style=\"width:15%;\" align=\"center\" >  \n                        <img width=\"90\" height=\"130\" src='{{datos_generales.foto_postulante}}'/>\n                    </td>\n                    <td align=\"center\" style=\"width:50%;\" > \n                        <h2><div>CURRICULUM VITAE</div><br></h2>\n                        <p class=\"estilo_texto_header\">{{curso.nombre_completo}} -- {{curso.metodologia}}</p>\n                        <p class=\"estilo_texto_header\">{{curso.metodologia}}</p>\n                    </td>\n                    <td style=\"width:20%;\" align=\"center\" >  \n                        <img width=\"90\" src='{{logos.logo}}'/>\n                    </td>\n                </tr>\n            </table>\n    \n    \n</div>\n\n<table style=\"width:100%;\">\n    <thead>\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            <td  colspan=\"4\">\n            DATOS GENERALES\n            </td>\n        </tr>\n    </thead>\n    <tbody>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nombres: </td>\n            <td>{{datos_generales.nombres}}</td>\n            <td style=\"font-weight: bold;\">Apellidos</td>\n            <td>{{datos_generales.apellidos}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">ID: </td>\n            <td>{{datos_generales.numero_id}}</td>\n            <td style=\"font-weight: bold;\">Sexo: </td>\n            <td>{{datos_generales.sexo}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nivel Formativo: </td>\n            <td>{{datos_generales.nivel_estudios}}</td>\n            <td style=\"font-weight: bold;\">Estado civil: </td>\n            <td>{{datos_generales.estado_civil}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Edad: </td>\n            <td>{{datos_generales.edad}}</td>\n            <td style=\"font-weight: bold;\">Telf. m\u00f3vil: </td>\n            <td >{{datos_generales.telefono_movil}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Email: </td>\n            <td>{{datos_generales.email}}</td>\n            <td style=\"font-weight: bold;\">Pa\u00eds: </td>\n            <td>{{datos_generales.pais}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nacionalidad: </td>\n            <td>{{datos_generales.nacionalidad}}</td>\n            <td style=\"font-weight: bold;\">Ciudad: </td>\n            <td >{{datos_generales.ciudad}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            <td colspan=\"4\">\n            FORMACION\n            </td>\n        </tr>\n        {%- for row in titulo -%}\n        <tr style=\"border-bottom:0pt solid #f00;font-size:12px\">\n            <td style=\"font-weight: bold;\">Titulo: </td>\n            <td>{{row.titulo}}</td>\n            <td style=\"font-weight: bold;\">Nivel: </td>\n            <td >{{row.nivel_formativo}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Instituci\u00f3n: </td>\n            <td>{{row.institucion}}</td>\n            <td style=\"font-weight: bold;\">Fecha: </td>\n            <td >{{row.fecha}}</td>\n        </tr>\n        {% endfor %}\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            <td colspan=\"4\">\n            CONOCIMIENTO\n            </td>\n        </tr>\n        {%- for row in conocimiento -%}\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Titulo: </td>\n            <td>{{row.titulo}}</td>\n            <td style=\"font-weight: bold;\">Horas: </td>\n            <td >{{row.horas}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Instituci\u00f3n: </td>\n            <td>{{row.institucion}}</td>\n            <td style=\"font-weight: bold;\">Fecha: </td>\n            <td >{{row.fecha}}</td>\n        </tr>\n        {% endfor %}\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            <td colspan=\"4\">\n            EXPERIENCIA LABORAL\n            </td>\n        </tr>\n        {%- for row in experiencia -%}\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Tipo Jornada: </td>\n            <td>{{row.tipo}}</td>\n            <td style=\"font-weight: bold;\">Cargo: </td>\n            <td >{{row.posicion}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Empresa/Instituci\u00f3n: </td>\n            <td>{{row.empresa}}</td>\n            <td style=\"font-weight: bold;\">Personal a cargo: </td>\n            <td >{{row.personas_acargo}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Fecha Ingreso/salida: </td>\n            <td>{{row.fecha_ingreso}}/{{row.fecha_salida}}</td>\n            <td style=\"font-weight: bold;\">Experiencia (en meses): </td>\n            <td >{{row.meses}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Descripci\u00f3n: </td>\n            <td colspan=3>{{row.descripcion}}</td>\n        </tr>\n        {% endfor %}\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            <td colspan=\"4\">\n            IDIOMAS\n            </td>\n        </tr>\n        {%- for row in idioma -%}\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Idioma: </td>\n            <td>{{row.idioma}}</td>\n            <td style=\"font-weight: bold;\">Lectura: </td>\n            <td >{{row.lectura}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid #ABB2B9;font-size:12px\">\n            <td style=\"font-weight: bold;\">Escrito: </td>\n            <td>{{row.escrito}}</td>\n            <td style=\"font-weight: bold;\">Hablado: </td>\n            <td >{{row.hablado}}</td>\n        </tr>\n        {% endfor %}\n    </tbody>\n</table>",
