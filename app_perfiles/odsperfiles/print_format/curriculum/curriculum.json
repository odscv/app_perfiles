 "html": "<meta name=\"pdfkit-page-size\" content=\"Legal\"/>\n<meta name=\"pdfkit-orientation\" content=\"portrait\"/>\n\n{%set datos_generales = frappe.get_doc(\"Datos Generales\",doc.id_participante) %}\n{%set curso = frappe.get_doc(\"Curso\",doc.codigocurso) %}\n{%set values = {'id_participante': doc.id_participante}%} \n\n <div class=\"print-heading\">\n    \n    <h2><div>CURRICULUM VITAE</div><br></h2>\n    <small class=\"sub-heading\"><div>{{curso.nombre_completo}} - {{curso.metodologia}}</div></small><br>\n    \n</div>\n\n<table style=\"width:100%;\">\n    <thead>\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            DATOS GENERALES\n        </tr>\n    </thead>\n    <tbody>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nombres: </td>\n            <td>{{datos_generales.nombres}}</td>\n            <td style=\"font-weight: bold;\">Apellidos</td>\n            <td>{{datos_generales.apellidos}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">ID: </td>\n            <td>{{datos_generales.numero_id}}</td>\n            <td style=\"font-weight: bold;\">Sexo: </td>\n            <td>{{datos_generales.sexo}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nivel Formativo: </td>\n            <td>{{datos_generales.nivel_estudios}}</td>\n            <td style=\"font-weight: bold;\">Estado civil: </td>\n            <td>{{datos_generales.estado_civil}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Edad: </td>\n            <td>{{datos_generales.edad}}</td>\n            <td style=\"font-weight: bold;\">Telf. m\u00f3vil: </td>\n            <td >{{datos_generales.telefono_movil}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Email: </td>\n            <td>{{datos_generales.email}}</td>\n            <td style=\"font-weight: bold;\">Pa\u00eds: </td>\n            <td>{{datos_generales.pais}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nacionalidad: </td>\n            <td>{{datos_generales.nacionalidad}}</td>\n            <td style=\"font-weight: bold;\">Ciudad: </td>\n            <td >{{datos_generales.ciudad}}</td>\n        </tr>\n        <tr style=\"border-bottom:1pt solid black;font-size:18px;\">\n            FORMACION\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Titulo: </td>\n            <td>{{formacion.titulo}}</td>\n            <td style=\"font-weight: bold;\">Ciudad: </td>\n            <td >{{datos_generales.ciudad}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Instituci\u00f3n: </td>\n            <td>{{datos_generales.nacionalidad}}</td>\n            <td style=\"font-weight: bold;\">Ciudad: </td>\n            <td >{{datos_generales.ciudad}}</td>\n        </tr>\n        <tr style=\"border-bottom:0pt solid black;font-size:12px\">\n            <td style=\"font-weight: bold;\">Nacionalidad: </td>\n            <td>{{datos_generales.nacionalidad}}</td>\n            <td style=\"font-weight: bold;\">Ciudad: </td>\n            <td >{{datos_generales.ciudad}}</td>\n        </tr>\n    </tbody>\n</table><br><br><br><br><br><br>\n<div style=\"text-align:center;line-heigth:0.5\">\n    <p>________________________________</p>\n    <p>Presidente</p>\n</div>",
